{
  "name": "workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "lint-staged": {
      "*.js": [
        "npm run lint:fix",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardoigor/precommit_husky.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leonardoigor/precommit_husky/issues"
  },
  "homepage": "https://github.com/leonardoigor/precommit_husky#readme",
  "devDependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4"
  }
}